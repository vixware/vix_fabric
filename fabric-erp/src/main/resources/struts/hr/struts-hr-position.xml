			<result name="goChoosePosition">/WEB-INF/content/hr/orgPosition/orgPosition_choose.jsp</result>
			<!-- <result name="goChoosePositionOrDep">/WEB-INF/content/hr/orgPosition/orgPositionOrDepartment_choose.jsp</result> -->
			<result name="addHrAttachments">/WEB-INF/content/hr/job/addAttachFile.jsp</result>
