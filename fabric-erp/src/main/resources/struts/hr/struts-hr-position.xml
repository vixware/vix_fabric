<struts>
	<package name="hr.position.config" namespace="/hr/position" extends="crud-default">
		 <!-- 岗位管理 -->
		 <action name="orgPositionAction!*" method="{1}" class="orgPositionAction">
			<result name="goList">/WEB-INF/content/hr/orgPosition/orgPosition_list.jsp</result>
			<result name="goSingleList">/WEB-INF/content/hr/orgPosition/orgPosition_list_content.jsp</result>
			<result name="goSaveOrUpdate">/WEB-INF/content/hr/orgPosition/orgPosition_update.jsp</result>
			<result name="goShowPosition">/WEB-INF/content/hr/orgPosition/show_orgPosition_update.jsp</result>
