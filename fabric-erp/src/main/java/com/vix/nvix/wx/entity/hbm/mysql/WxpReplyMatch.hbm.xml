<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.vix.nvix.wx.entity.WxpReplyMatch" table="wxp_reply_match">
		<id name="id" type="java.lang.String" column="id">
			<generator class="uuid2">
				<param name="uuid_gen_strategy_class">org.hibernate.id.uuid.CustomVersionOneStrategy</param>
			</generator>
		</id>

		<property name="appId" type="java.lang.String" column="app_id" update="false" />
		<property name="updateTime" type="java.util.Date" column="UPDATETIME" />
		<property name="createTime" type="java.util.Date" column="CREATETIME" update="false" />
		<property name="tenantId" type="java.lang.String" column="tenant_id" update="false" />

		<property name="keyword" type="java.lang.String" column="keyword" />
		<property name="fullMatch" type="java.lang.Integer" column="full_match" />
		<property name="isActive" type="java.lang.Integer" column="is_active" />
		<many-to-one name="replyRule" column="wxp_reply_rule_id" />
	</class>
</hibernate-mapping>