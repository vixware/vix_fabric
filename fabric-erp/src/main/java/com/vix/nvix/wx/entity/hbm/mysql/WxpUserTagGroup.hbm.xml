<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.vix.nvix.wx.entity.WxpUserTagGroup" table="wxp_user_tag_group">
		<id name="id" type="java.lang.String" column="id">
			<generator class="uuid2">
				<param name="uuid_gen_strategy_class">org.hibernate.id.uuid.CustomVersionOneStrategy</param>
			</generator>
		</id>
		<property name="appId" type="java.lang.String" column="app_id" update="false" />

		<property name="updateTime" type="java.util.Date" column="UPDATETIME" />
		<property name="createTime" type="java.util.Date" column="CREATETIME" update="false" />
		<property name="tenantId" type="java.lang.String" column="tenant_id" update="false" />

		<property name="title" type="java.lang.String" column="title" />
		<property name="isMutual" type="java.lang.Integer" column="is_mutual" />
		<property name="otherPlatform" type="java.lang.String" column="other_platform" />

	</class>
</hibernate-mapping>