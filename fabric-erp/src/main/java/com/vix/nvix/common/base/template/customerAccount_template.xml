<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
	<worksheet name="customerAccount">
		<section startRow="0" endRow="0" />
		<loop startRow="1" endRow="1" items="customerAccountList" var="customerAccount" varType="com.vix.mdm.crm.entity.CustomerAccount">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0">customerAccount.code</mapping>
				<mapping row="1" col="1">customerAccount.name</mapping>
				<mapping row="1" col="2">customerAccount.mobilePhone</mapping>
				<mapping row="1" col="3">customerAccount.sex</mapping>
				<mapping row="1" col="4">customerAccount.age</mapping>
				<mapping row="1" col="5">customerAccount.address</mapping>
				<mapping row="1" col="6">customerAccount.birthday</mapping>
				<mapping row="1" col="7">customerAccount.money</mapping>
				<mapping row="1" col="8">customerAccount.point</mapping>
				<mapping row="1" col="9">customerAccount.isUse</mapping>
				<mapping row="1" col="10">customerAccount.qqAccount</mapping>
				<mapping row="1" col="11">customerAccount.msnAccount</mapping>
				<mapping row="1" col="12">customerAccount.email</mapping>
				<mapping row="1" col="13">customerAccount.referrerPhone</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0" />
				<rowcheck offset="1" />
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>