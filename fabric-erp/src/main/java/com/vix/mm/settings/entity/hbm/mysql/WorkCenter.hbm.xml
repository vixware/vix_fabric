			<column name="NUMBERDAILYFLIGHTS" length="254" />
		</property>
		<property name="handlingTime" type="java.lang.Double" column="handlingTime" />
		<property name="machinesNumber" type="java.lang.String">
			<column name="MACHINESNUMBER" length="254" />
		</property>
		<property name="establishmentUser" type="java.lang.String">
			<column name="ESTABLISHMENTUSER" length="254" />
		</property>
		<property name="creDate" type="java.util.Date">
