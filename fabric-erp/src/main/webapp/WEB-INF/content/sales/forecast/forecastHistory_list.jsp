<script type="text/javascript">
$('input.btn[type="button"],input.btn[type="submit"]').hover(function(){
	$(this).addClass("btnhover");
},function(){
	$(this).removeClass("btnhover");
});

function searchForContent(){
	loadCode();
	pager("start","${vix}/sales/forecast/forecastHistoryAction!goListContent.action?code=",'forecast');
}
 
//载入分页列表数据
pager("start","${vix}/sales/forecast/forecastHistoryAction!goListContent.action?code="+"&pageNo=${pageNo}",'forecast');
bindSearch();
function currentPager(tag){
	pager(tag,"${vix}/sales/forecast/forecastHistoryAction!goListContent.action?code=",'forecast');
}
$(".drop>ul>li").bind('mouseover',function(){
	$(this).children('ul').delay(400).slideDown('fast');
}).bind('mouseleave',function(){
	$(this).children('ul').slideDown('fast').stop(true, true);
	$(this).children('ul').slideUp('fast');
});
</script>
