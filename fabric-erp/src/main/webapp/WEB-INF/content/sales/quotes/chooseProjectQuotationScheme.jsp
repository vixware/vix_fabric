//排序 
function orderBy(orderField){
	loadQuoteSubject();
	var orderBy = $("#subSalesQuotationOrderBy").val();
	if(orderBy == 'desc'){
		orderBy = "asc";
	}else{
		orderBy = 'desc';
	}
	pager("start","${vix}/sales/quotes/salesQuotationAction!goSubListContent.action?orderField="+orderField+"&orderBy="+orderBy+"&quoteSubject="+quoteSubject,'subSalesQuotation');
}

bindSearch();
function currentPager(tag){
	loadQuoteSubject();
	pager(tag,"${vix}/sales/quotes/salesQuotationAction!goSubListContent.action?quoteSubject="+quoteSubject,'subSalesQuotation');
}
 
</script>
